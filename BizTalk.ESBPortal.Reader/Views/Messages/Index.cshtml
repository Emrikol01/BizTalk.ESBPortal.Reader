            @Html.DisplayFor(modelItem => item.RoutingUrl)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.ResubmitAttempted)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.ResubmitSuccessful)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.InsertedDate)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.ActivityID)
        </td>
        <td>
            @Html.ActionLink("Edit", "Edit", new { id=item.MessageID }) |
            @Html.ActionLink("Details", "Details", new { id=item.MessageID }) |
            @Html.ActionLink("Delete", "Delete", new { id=item.MessageID })
        </td>
    </tr>
}

</table>
