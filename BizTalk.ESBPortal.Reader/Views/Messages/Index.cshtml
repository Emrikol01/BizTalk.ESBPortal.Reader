@model IEnumerable<BizTalk.ESBPortal.Reader.Message>

@{
    ViewBag.Title = "Index";
}

<h2>Index</h2>

<p>
    @Html.ActionLink("Create New", "Create")
</p>
<table class="table">
    <tr>
        <th>
            @Html.DisplayNameFor(model => model.NativeMessageID)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.FaultID)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.ContentType)
