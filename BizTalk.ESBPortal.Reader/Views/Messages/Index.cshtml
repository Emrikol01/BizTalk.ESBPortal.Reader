        <th>
            @Html.DisplayNameFor(model => model.RoutingUrl)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.ResubmitAttempted)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.ResubmitSuccessful)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.InsertedDate)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.ActivityID)
        </th>
        <th></th>
    </tr>

@foreach (var item in Model) {
    <tr>
        <td>
            @Html.DisplayFor(modelItem => item.NativeMessageID)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.FaultID)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.ContentType)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.MessageName)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.InterchangeID)
        </td>
